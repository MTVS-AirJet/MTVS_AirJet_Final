prefix=${pcfiledir}/../..
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: absl_overload
Description: Abseil overload library
URL: https://abseil.io/
Version: 20240116

Libs: "-L${libdir}"
Requires: absl_meta = 20240116
Cflags: "-I${includedir}" -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
