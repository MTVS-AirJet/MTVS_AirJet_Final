prefix=${pcfiledir}/../..
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: absl_absl_check
Description: Abseil absl_check library
URL: https://abseil.io/
Version: 20240116

Libs: "-L${libdir}"
Requires: absl_log_internal_check_impl = 20240116
Cflags: "-I${includedir}" -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
